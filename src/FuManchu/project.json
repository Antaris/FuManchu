{
  "version": "1.5.0",
  "description": "FuManchu is a text templating engine based on HandlebarsJS",
  "title": "FuManchu - Handlebar templating for .NET",

  "packOptions": {
    "tags": [
      "fumanchu",
      "handlebars"
    ],
    "repository": {
      "type": "git",
      "url": "git://github.com/Antaris/FuManchu"
    }
  },

  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [ "CS1591"]
  },

  "dependencies": {
    "NETStandard.Library": "1.5.0-rc2-24027"
  },

  "frameworks": {
    "net451": { },
    "netstandard1.5": {
      "imports": [
        "dnxcore50",
        "portable-net452+win81"
      ],
      "dependencies": {
        "System.Dynamic.Runtime": "4.0.11-rc2-24027",
        "System.Linq.Expressions": "4.0.11-rc2-24027"
      }
    }
  }
}